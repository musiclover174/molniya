.content_object
  pos relative
  z-index 1
  background-color black
  .breadcrumbs,
  .breadcrumbs + h1:not([class])
    pos relative
    z-index 1
    color white
  .breadcrumbs__href
    color white
  & > .container
    display flex
    flex-direction column
  &.change
    .objects__content
      l -20px
      opacity 0
    .objects__bg
      opacity 0
  & + .footer
    mt 0

.object
  &__top
    h calc(100vh - 200px)
    min-height 600px
    pb 70px
    @media $lDesktopSmall
      h calc(100vh - 157px)
    @media $desktopMedium
      display flex
      align-items stretch
      h auto
      min-height inherit
    &-upper
      @media $desktopMedium
        mb 100px
    & > .container
      display flex
      flex-direction column
      align-items flex-start
      justify-content space-between
      h 100%
      @media $desktopMedium
        h auto
  &__bottom
    bgc white
  &__top,
  &__bottom
    pos relative
    z-index 1
  &__sub
    display flex
    flex-wrap wrap
    @media $LandscapeMax
      flex-direction column
    &-address
      pos relative
      color white
      fontify 17px 300 25px
      pl 24px
      @media $lDesktopSmall
        fontify 15px 300 25px
      &:before
        content ""
        pos absolute
        l 0
        t 2px
        size 13px 18px
        background url("data:image/png;base64,") no-repeat center
    &-text
      color white
      fontify 17px 300 25px
      ml 90px
      @media $lDesktopSmall
        fontify 15px 300 25px
      @media $LandscapeMax
        ml 0
        mt 10px
  &__info
    display flex
    justify-content space-between
    w 100%
    pt 35px
    border-top 1px solid alpha(white, .1)
    @media $desktopMax
      pt 0
    @media $desktopMin
      flex-direction column
    &-col
      w calc(50% - 31px)
      display flex
      align-items flex-start
      justify-content flex-start
      flex-wrap wrap
      @media $desktopMax
        w calc(50% - 20px)
      @media $desktopMedium
        w auto
        flex-grow 1
      & + &
        ml 62px
        @media $desktopMax
          ml 40px
        @media $desktopMedium
          flex-shrink 0
          w 255px
        @media $desktopMin
          ml 0
          mt 60px
      &_right
        display flex
        align-items flex-end
        justify-content flex-end
        .objects__order
          max-width 375px
          @media $lDesktopSmall
            max-width 255px
    &-elem
      w 50%
      pr 31px
      mt 30px
      @media $PortraitMax
        w 100%
        pr 0
    &-title
      color alpha(white, .5)
      fontify 17px 300 25px
      mb 13px
      @media $lDesktopSmall
        fontify 15px 300 25px
        mb 9px
    &-value
      color white
      fontify 31px 100 31px
  &__title
    fontify 36px 900 38px
    max-width 572px
    mb 40px
    @media $LandscapeMax
      fontify 26px 900 28px
    &_white
      color white
  &__about
    pt 77px
    mb 73px
    &-row
      display flex
      align-items flex-start
      @media $LandscapeMax
        flex-direction column
      & + &
        mt 30px
        @media $LandscapeMax
          mt 40px
    &-label
      color alpha(black, .5)
      w 265px
      pr 30px
      fontify 17px 300 25px
      @media $lDesktopSmall
        fontify 15px 300 23px
      @media $LandscapeMax
        w 100%
        pr 0
        mb 15px
    &-text
      w calc(100% - 265px)
      @media $LandscapeMax
        w 100%
  &__gallery
    mb 35px
  &__gal
    pos relative
    l 50%
    w 100vw
    ml -50vw
    h calc(100vh - 40px)
    .swiper-slide
      background-size cover
      background-position center
      background-repeat no-repeat
    &-over
      .swiper-buttons
        w 100%
        max-width 1600px
        margin -40px auto 0
        padding 0 80px
        text-align right
        font-size 0
        @media $lDesktopSmall
          mt -35px
        @media $LandscapeMax
          mt -30px
        @media $PortraitMax
          text-align center
        &_center
          text-align center
        @media $desktopMedium
          padding 0 30px
        @media $LandscapeMedium
          padding 0 20px
      .swiper-button-prev, 
      .swiper-button-next
        bgc $red
        size 80px
        &:hover
          bgc #c91a20
          background-image url("data:image/png;base64,")
        @media $lDesktopSmall
          size 70px
        @media $LandscapeMax
          size 60px
      .swiper-button-next
        ml 0
      
      
  &__feats
    mb 20px
    &-list
      display flex
      flex-wrap wrap
      justify-content space-between
      pt 20px
      @media $lDesktopSmall
        pt 10px
    &-item
      pos relative
      w calc(50% - 31px)
      pl 70px
      mb 50px
      @media $desktopMedium
        w calc(50% - 20px)
      @media $desktopMin
        w 100%
      &-title
        color $red
        mb 16px
        fontify 20px 700 28px
        @media $lDesktopSmall
          fontify 18px 700 28px
        @media $desktopMedium
          fontify 15px 700 23px
      &-text
        fontify 21px 300 29px
        @media $lDesktopSmall
          fontify 18px 300 28px
        @media $desktopMedium
          fontify 15px 300 23px
      &[data-type="location"]:before
        content ""
        pos absolute
        l 0
        t -9px
        size 46px
        background url("data:image/png;base64,") no-repeat center
      &[data-type="infrastructure"]:before
        content ""
        pos absolute
        l 0
        t -7px
        size 43px
        background url("data:image/png;base64,") no-repeat center
      &[data-type="new"]:before
        content ""
        pos absolute
        l 0
        t -8px
        size 41px 46px
        background url("data:image/png;base64,") no-repeat center
      &[data-type="equipment"]:before
        content ""
        pos absolute
        l 0
        t -9px
        size 49px
        background url("data:image/png;base64,") no-repeat center
  &__layout
    mb 72px
    &-map
      max-width 100%
  &__map
    pos relative
    h 60vh
    w 100vw
    l 50%
    ml -50vw
    &-over
      mb 68px
      @media $lDesktopSmall
        mb 88px
      @media $LandscapeMax
        mb 60px
  &__manager
    .contacts__managers
      pb 0
      mb 70px
  &__lease
    pos relative
    overflow hidden
    w 100vw
    ml -50vw
    pos relative
    l 50%
    bgc $red
    padding 120px 0
    @media $lDesktopSmall
      padding 75px 0
    .objects__bg
      opacity .08
    .container
      pos relative
      z-index 1
    &-sub
      color white
      fontify 17px 300 25px
      mb 37px
    &-form
      &-top,
      &-bottom
        display flex
        justify-content space-between
      &-bottom
        flex-direction column
        align-items flex-start
      &-top
        @media $LandscapeMax
          flex-direction column
          align-items flex-start
      .fakecheckbox
        mt 35px
      input.warning,
      textarea.warning
        border-color #bf0209
    &-col
      pos relative
      w calc(50% - 30px)
      @media $desktopMedium
        w calc(50% - 15px)
      @media $LandscapeMax
        w 100%
        & + &
          mt 22px
    &-row
      display flex
      justify-content space-between
      pos relative
      w 100%
      @media $LandscapeMax
        flex-direction column
      & + &
        mt 34px
        @media $LandscapeMax
          mt 22px
    &-sbm
      db()
      pos relative
      font-family $os
      mt 30px
      color white
      fontify 13px 700 36px
      cursor pointer
      text-align left
      transition .7s background
      &:after
        content ""
        display inline-block
        vertical-align middle
        pos relative
        r 0
        ml 10px
        size 35px 19px
        background url("data:image/png;base64,") no-repeat center
        transition .7s right
      &:hover
        &:after
          r -5px
  &__others
    padding 80px 0 110px
    @media $lDesktopSmall
      pb 80px
    &-car
      mb 60px
      &.swiper-container
        overflow visible
        @media $desktopMin
          overflow hidden
      .swiper-buttons
        pos absolute
        r 0
        t -75px
        @media $desktopMin
          pos inherit
          t 0
          r auto
          text-align center
          mt 30px
      .swiper-button-next
        mr 0
    &-allover
      text-align center
    &-all
      color black
      fontify 21px 300 21px
      border-bottom 1px solid
      transition .7s border
      @media $lDesktopSmall
        fontify 15px 300 15px
      &:hover
        border-color transparent
    &-imgover
      w 100%
      pt 58%
      background-repeat no-repeat
      background-size cover
      background-position center
    &-content
      display flex
      flex-direction column
      align-items flex-start
      margin -76px 28px 0
      bgc $red
      padding 30px 35px
      @media $lDesktopSmall
        padding 26px 29px 23px 29px
        margin -56px 24px 0
      @media $PortraitMax
        margin 0
    &-name
      color white
      fontify 21px 700 23px
      border-bottom 2px solid transparent
      transition .7s border
      @media $lDesktopSmall
        fontify 18px 700 23px
      &:hover
        border-color white
    &-place
      color alpha(white, .5)
      fontify 19px 300 25px
      mt 15px
      pos relative
      pl 26px
      @media $lDesktopSmall
        fontify 15px 300 25px
        mt 6px
      &:before
        content ""
        pos absolute
        l 0
        t 0
        opacity .5
        size 14px 20px
        background url("data:image/png;base64,") no-repeat center