.ibanner
  min-height 605px
  h 70vh
  @media $desktopMin
    min-height 555px
  @media $LandscapeMax
    min-height 455px
  @media $PortraitMedium 
    min-height calc(100vh - 88px)
  .swiper-slide
    bgc #060606
    z-index 1
    overflow hidden
    display flex
    align-items flex-end
    pb 230px
    @media $LandscapeMax
      pb 110px
    &-active
      & ^[0]
        &__img
          transform translate(-50%, -50%) scale(1.08)
  .swiper-buttons
    pos absolute
    z-index 1
    b 137px
    l 50%
    w 100%
    max-width 1600px
    padding 0 80px
    transform translateX(-50%)
    @media $desktopMedium
      padding 0 30px
    @media $LandscapeMax
      b 37px
  .swiper-progress
    pos absolute
    z-index 1
    b 80px
    h 7px
    w 0
    bgc $red
    transition .7s width
    @media $LandscapeMax
      b 0
  &__img
    opacity .45
    z-index -1
    imgCenteringMin()
    transition 7s transform
  &__text
    color white
    fontify 72px 900 76px
    @media $desktopMax
      max-width 75%
    @media $lDesktopSmall
      fontify 60px 900 64px
    @media $desktopMax
      fontify 46px 700 50px
    @media $desktopMin
      fontify 36px 700 42px
    @media $LandscapeMax
      fontify 30px 700 36px
      max-width none
    @media $PortraitMedium
      fontify 24px 700 28px
    
.iabout
  //bgc white
  pt 92px
  mb -90px
  pos relative
  z-index 1
  @media $desktopMin
    mb -30px
  @media $desktopMin
    mb 0
  &__title
    fontify 60px 900 52px
    mb 43px
    @media $lDesktopSmall
      fontify 48px 900 52px
  &__row
    display flex
    justify-content space-between
    @media $desktopMin
      flex-direction column
      justify-content flex-start
      align-items flex-start
  &__text
    fontify 18px 300 28px
    max-width 490px
    @media $desktopMax
      max-width 350px
    @media $desktopMin
      max-width none
    &-over
      w calc(50% - 31px)
      @media $desktopMin
        w 100%
  &__imgover
    w calc(50% - 31px)
    mt 70px
    @media $desktopMin
      w 100%
      mt 30px
  &__img
    pos relative
    l -62px
    max-width 100%
    @media $desktopMin
      l 0
  &_bgfixed
    pos relative
    z-index 0
    bgc white
    mb 57px
    .container
      pos relative
      h 810px
      display flex
      align-items center
      @media $desktopMedium
        h 610px
      @media $desktopMin
        h auto
        margin 50px 0
        padding 0
      &:before
        content ""
        background url('../i/about-mask.png') no-repeat center
        pos absolute
        t 0
        l 0
        size 100% 100%
        z-index 1
        @media $desktopMedium
          background-size cover
        @media $desktopMin
          dn()
      &:after
        content ""
        background url('../i/about-bgfixed.jpg') no-repeat center
        pos absolute
        t 0
        l 0
        size 100% 100%
        z-index 0
        background-attachment fixed
        @media $desktopMin
          dn()
  &__we
    fontify 18px 300 28px
    max-width 425px
    pos relative
    z-index 2
    @media $lDesktopMedium
      max-width 350px
    @media $lDesktopSmall
      max-width 270px
    @media $desktopMin
      max-width none
      padding 0 30px
  &__spar
    mb 152px
    @media $desktopMin
      mb 70px
    .container
      display flex
      justify-content space-between
      align-items center
      @media $desktopMin
        flex-direction column-reverse
        justify-content flex-start
        align-items flex-start
    &-textover,
    &-imgover
      w calc(50% - 31px)
      @media $desktopMin
        w 100%
    &-text
      fontify 28px 500 38px
      max-width 600px
      @media $lDesktopMedium
        max-width 500px
      @media $lDesktopSmall
        max-width 410px
      @media $desktopMax
        fontify 24px 500 34px
      @media $desktopMin
        fontify 18px 300 28px
      @media $desktopMin
        max-width none
        mt 30px
    &-img
      max-width 100%
  &__feats
    mb 85px
    @media $desktopMin
      mb 25px
    &-list
      display flex
      flex-wrap wrap
      ml -62px
      w calc(100% + 62px)
    &-item
      w calc(50% - 62px)
      ml 62px
      pos relative
      pl 60px
      fontify 21px 300 31px
      mb 69px
      pr 110px
      @media $lDesktopMedium
        fontify 18px 300 24px
      @media $desktopMin
        w calc(100% - 62px)
        pr 0
      span
        font-weight 700
        color $red
      &:before
        content ""
        pos absolute
        l 0
        t -12px
      &[data-icon="tree"]:before
        size 38px 47px
        l -4px
        t -5px
        @media $lDesktopMedium
          t 0
        @media $desktopMin
          t -12px
        background url("data:image/png;base64,") no-repeat center
      &[data-icon="map"]:before
        size 46px
        t -5px
        l -6px
        @media $lDesktopMedium
          t 0
          l -10px
        background url("data:image/png;base64,") no-repeat center
      &[data-icon="person"]:before
        size 33px 47px
        background url("data:image/png;base64,") no-repeat center
      &[data-icon="family"]:before
        size 50px 46px
        l -8px
        @media $lDesktopMedium
          l -10px
        background url("data:image/png;base64,") no-repeat center
.iclients
  &__spacer
    h 100vh
  &__over
    pos relative
  &__scenes
    pos absolute
    t 0
    l 0
    w 100%
    h 100vh
    &.scene1
      & ^[0]__scene
        &[data-step="1"]
          z-index 1
          opacity 1
          visibility visible
          .iclients__variant 
            opacity 1
            top 0
            transition-delay 1s
          .iclients__scene-bg
            transform scale(1)
      .iclients__scene-title
        t 71px
        opacity 1
        transition-delay 1s
        @media $desktopMedium
          t 31px
        @media $desktopMin
          t 116px
        @media $PortraitMedium 
          t 108px
    &.scene2
      & ^[0]__scene
        &[data-step="1"]
          z-index 1
          .iclients__variant 
            opacity 0
            top -60px
          .iclients__scene-bg
            transform scale(1.2)
        &[data-step="2"]
          z-index 2
          opacity 1
          visibility visible
          .iclients__scene-bg
            transform scale(1)
          .iclients__variant 
            opacity 1
            top 0
            transition-delay 1s
    &.scene3
      & ^[0]__scene
        &[data-step="2"]
          z-index 1
          .iclients__variant 
            opacity 0
            top -60px
          .iclients__scene-bg
            transform scale(1.2)
        &[data-step="3"]
          z-index 2
          opacity 1
          visibility visible
          .iclients__scene-bg
            transform scale(1)
          .iclients__more
            t 0 
            opacity 1
            transition-delay 1s
          .iclients__car-over
            t 0 
            opacity 1
            transition-delay 1s
      .iclients__scene-title
        t 41px
        opacity 0
        transition-delay 0s
    &.scene4
      & ^[0]__scene
        &[data-step="3"]
          z-index 1
          .iclients__scene-bg
            transform scale(1.2)
          .iclients__more
            t -40px 
            opacity 0
          .iclients__car-over
            t -40px 
            opacity 0
        &[data-step="4"]
          z-index 2
          opacity 1
          visibility visible
          .iclients__scene-bg
            transform scale(1)
          .iclients__variant 
            opacity 1
            top 0
            transition-delay 1s
          .iclients__contacts
            opacity 1
            b 56px
            transition-delay 1s
  &__scene
    pos absolute
    t 0
    l 0
    w 100%
    h 100%
    display flex
    align-items center
    justify-content center
    bgc #060606
    z-index 2
    overflow hidden
    opacity 0
    visibility hidden
    transition .7s opacity, .7s visibility
    @media $desktopMin
      pt 116px
    @media $PortraitMedium 
      pt 108px
    @media $LandscapeMax
      justify-content flex-start
      pl 30px
      pr 30px
    &[data-step="1"],
    &[data-step="2"]
      @media $LandscapeMedium
        align-items flex-end
        pb 40px
    &[data-step="4"]
      @media $desktopMin
        pt 156px
      @media $LandscapeMedium
        align-items flex-start
      @media $PortraitMedium 
        pt 148px
    &:first-child
      opacity 1
      visibility visible
    &-bg
      pos absolute
      t 0 
      l 0
      w 100%
      h 100%
      opacity .45
      z-index -1
      background-position center
      background-size cover
      background-attachment fixed
      transition 2s transform
      transform scale(1.2)
    &:first-child &-bg
      transform scale(1)
    &-title
      pos absolute
      z-index 2
      l 80px
      r 80px
      t 111px
      opacity 0
      transition .7s opacity, .7s top
      @media $desktopMedium
        l 30px
        t 71px
      @media $desktopMin
        t 156px
      @media $PortraitMedium 
        t 148px
      & > .container
        padding 0
    & > .container
      h 100%
      display flex
      flex-direction column
      justify-content space-between
      align-items flex-start
      @media $LandscapeMax
        padding 0
  &__title
    pos relative
    color white
    fontify 36px 900 40px
    @media $lDesktopSmall
      fontify 24px 900 28px
  &__progress
    pos absolute
    z-index 3
    t 0
    r 80px
    h 0
    w 7px
    bgc $red
    opacity 0
    transition .7s opacity
    &[style]
      opacity 1
    @media $desktopMedium
      r 30px
    @media $LandscapeMax
      r 0
  &__variant
    pos relative
    max-width 572px
    bgc $red
    padding 54px 48px 30px
    color white
    fontify 15px 300 25px
    opacity 0
    t 60px
    transition .7s opacity, .7s top
    @media $LandscapeMax
      l 25px
      w calc(100% - 25px)
      max-width none
    @media $PortraitMin
      l 0
      w 100%
      padding 29px 28px 20px
    &-img
      pos absolute
      l -62px
      b 90%
      @media $LandscapeMax
        l -25px
      @media $PortraitMin
        max-width 150px
        b 95%
        l 50%
        transform translateX(-50%)
    &-title
      pos absolute
      l -62px
      b 90%
      color white
      fontify 48px 900 52px
      w 1px
      @media $lDesktopSmall
        fontify 36px 900 40px
      @media $LandscapeMax
        l -25px
      @media $LandscapeMax
        fontify 30px 900 36px
      @media $PortraitMedium
        fontify 24px 900 28px
        l 28px
        b 95%
  &__feed
    cursor pointer
    pos relative
    color white
    fontify 13px 700 13px
    &:after
      content ""
      pos absolute
      t -2px
      r -45px
      size 35px 19px
      background url("data:image/png;base64,") no-repeat center
      transition .7s right
    &:hover:after
      r -55px
    &-over
      text-align right
      mt 23px
      pr 28px
  &__contacts
    display flex
    flex-direction column
    pos absolute
    l 80px
    b 16px
    opacity 0
    transition .7s opacity, .7s bottom
    @media $desktopMedium
      l 30px
      b 20px
    &-title
      color white
      fontify 15px 300 19px
      mb 14px
    &-phone
      color white
      fontify 21px 500 21px
      @media $lDesktopSmall
        fontify 18px 500 18px
  &__more
    pos relative
    color white
    fontify 36px 900 40px
    pt 70px
    t 40px
    opacity 0
    transition .7s top, .7s opacity
    @media $lDesktopSmall
      fontify 24px 900 28px
    @media $desktopMedium
      pt 30px
    @media $desktopMin
      pt 0
  &__car
    &-over
      w 100%
      pos relative
      opacity 0
      t 20px
      transition .7s opacity, .7s top
      .swiper-buttons
        pos absolute
        l 0
        t -75px
    .swiper-wrapper
      display flex
      align-items center
    .swiper-slide
      mb 75px
      @media $LandscapeMedium
        mb 35px
  &__img
    max-width 80%